{
  "name": "cryptopunk",
  "version": "0.1.0",
  "author": "Jither",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "build": "webpack",
    "build-benchmarks": "webpack --config webpack.benchmarks.config.js",
    "deploy": "webpack -p",
    "dev": "webpack --watch",
    "sandbox": "webpack --watch --config webpack.sandbox.config.js",
    "vectors": "webpack --config webpack.vectors.config.js && victor vectors --transforms vectors/transforms.js"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-preset-env": "^1.6.0",
    "benchmark": "^2.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "figures": "^2.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": {
      "babelrc": true,
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "src/scripts-es6"
            ]
          }
        ]
      ]
    },
    "files": [
      "tests/**/*.js"
    ]
  }
}

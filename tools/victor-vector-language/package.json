{
    "name": "victor-vector-language",
    "displayName": "Victor Vector Language",
    "description": "Language support for Victor's test vectors syntax",
    "version": "0.0.1",
    "publisher": "Jither",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "victor",
            "aliases": ["Victor", "victor"],
            "extensions": [".vectors"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "victor",
            "scopeName": "source.vectors",
            "path": "./syntaxes/victor.tmLanguage.json"
        }]
    },
    "scripts": {
        "install": "node install"
    }
}